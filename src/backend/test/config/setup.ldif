#
# Use this data to run the JUnit tests
#
#
# Add this to the end of your slapd.conf to create a separate database
# for Jamm testing:
#
# ----- slap.conf -----
# database      ldbm
# suffix        "dc=jamm,dc=test"
# rootdn        "cn=Manager,dc=jamm,dc=test"
# rootpw        jammtest
# directory     /var/lib/ldap/jamm
# access to * by * read
# ----- slapd.conf -----
#

dn: dc=jamm, dc=test
objectClass: top

dn: cn=Manager, dc=jamm, dc=test
objectClass: top
objectClass: organizationalRole
cn: Manager

dn: o=hosting, dc=jamm, dc=test
objectClass: top
objectClass: organization
o: hosting

dn: virtualDomain=example1.test, o=hosting, dc=jamm, dc=test
objectClass: top
objectClass: JammVirtualDomain
virtualDomain: example1.test
postfixTransport: virtual:

dn: cn=postmaster, virtualDomain=example1.test, o=hosting, dc=jamm, dc=test
objectClass: top
objectClass: organizationalRole
objectClass: JammMailAlias
cn: postname
mail: postmaster@domain1.test
maildrop: postmaster
